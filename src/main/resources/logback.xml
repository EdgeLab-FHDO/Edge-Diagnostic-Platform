<configuration>
    <appender name="STDOUT"
              class="ch.qos.logback.core.ConsoleAppender">
        <!-- encoders are assigned the type ch.qos.logback.classic.encoder.PatternLayoutEncoder
            by default -->
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-7level %logger{35} - %msg%n
            </pattern>
        </encoder>
    </appender>

    <logger
            name="InfrastructureManager.Modules.MatchMaking.ScoreBased.ScoreBasedMatchMaking"
            level="OFF"/>
    <logger
            name="InfrastructureManager.MatchMaking.MatchMaker"
            level="OFF"/>
    <logger
            name="InfrastructureManager.Modules.MatchMaking.ScoreBased.ScoreBasedMatchMaking"
            level="INFO"/>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>


</configuration>